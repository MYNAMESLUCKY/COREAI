# Example configuration for Yogz
# Copy this file to .env and fill in your values

# Tavily API key (optional, for web search)
# Get a key at https://tavily.com
TAVILY_API_KEY=YOUR_TAVILY_API_KEY

# Ollama settings
OLLAMA_HOST=http://localhost:11434
OLLAMA_MODEL=deepseek-v3.1:671b-cloud

# Filesystem tools (dangerous when enabled)
AGENT_ENABLE_FS=false
AGENT_ALLOW_DIRS=.

# Python service URL (usually auto-managed by supervisor; override only for dev)
PYTOOLS_URL=http://127.0.0.1:8787
